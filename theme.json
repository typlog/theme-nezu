{
  "name": "nezu",
  "name#ja": "根津",
  "name#zh": "根津",
  "version": "0.3.0",
  "repo": "typlog/theme-nezu",
  "image": "https://user-images.githubusercontent.com/290496/185733933-ed6ad0a0-0634-47bd-8a45-2e38e1d8b5ae.png",
  "description": "A single column theme for blogs and podcasts.",
  "context": {
    "_static_url": "/src",
    "theme_color_mode": "auto",
    "theme_nav_links": [
      {
        "title": "Examples",
        "link": "#",
        "children": [
          {
            "icon": "logos:amp-icon",
            "title": "Blog",
            "subtitle": "A blog example",
            "link": "/blog/"
          },
          {
            "icon": "lucide:podcast",
            "title": "Podcast",
            "subtitle": "A podcast example",
            "link": "/podcast/"
          }
        ]
      },
      {
        "title": "GitHub",
        "link": "https://github.com/typlog/theme-ueno",
        "new_tab": true
      }
    ],
    "theme_social_links": [
      {
        "icon": "logos:github-icon",
        "title": "GitHub",
        "link": "https://github.com/typlog/theme-nezu"
      },
      {
        "icon": "logos:bluesky",
        "title": "Bluesky",
        "link": "https://bsky.app/profile/typlog.com"
      }
    ],
    "theme_podcast_links": [
      {
        "icon": "simple-icons:applepodcasts",
        "title": "Apple Podcasts",
        "link": "https://podcasts.apple.com/us/podcast/typlog/id1564550000"
      },
      {
        "icon": "simple-icons:spotify",
        "title": "Spotify",
        "link": "https://open.spotify.com/show/5Z6Z9Z6Z6Z6Z6Z6Z6Z6Z6Z"
      }
    ],
    "theme_copyright": "Copy &copy; 2024, <a href='https://typlog.com/'>Typlog</a>"
  },
  "config": [
    {
      "name": "color_mode",
      "type": "enum",
      "default": "auto",
      "options": {
        "choices": ["auto", "light", "dark"]
      }
    },
    {
      "name": "copyright",
      "type": "string"
    },
    {
      "name": "nav_links",
      "type": "NavLink",
      "multiple": true,
      "options": {
        "children": true
      }
    },
    {
      "name": "podcast_links",
      "type": "IconLink",
      "multiple": true
    },
    {
      "name": "social_links",
      "type": "IconLink",
      "multiple": true
    },
    {
      "name": "foot_links",
      "type": "SectionLinks",
      "multiple": true
    }
  ],
  "messages": {
    "none": "There is no items",
    "none#zh": "没有内容",
    "archive": "Archive",
    "archive#zh": "归档",
    "prev": "Previous",
    "prev#zh": "上一页",
    "next": "Next",
    "next#zh": "下一页",
    "readthis": "Read This",
    "readthis#zh": "下一篇",
    "switch_auto": "Switch to auto color mode",
    "switch_auto#zh": "切换至系统默认模式",
    "switch_light": "Switch to light color mode",
    "switch_light#zh": "切换至明亮模式",
    "switch_dark": "Switch to dark color mode",
    "switch_dark#zh": "切换至黑暗模式",
    "copy": "Copy",
    "copy#zh": "复制"
  }
}
