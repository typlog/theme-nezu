{
  "name": "nezu",
  "name#ja": "根津",
  "name#zh": "根津",
  "version": "0.1.0",
  "repo": "typlog/theme-nezu",
  "image": "https://user-images.githubusercontent.com/290496/185733933-ed6ad0a0-0634-47bd-8a45-2e38e1d8b5ae.png",
  "description": "A single column theme for blogs and podcasts.",
  "context": {
    "_static_url": "/src",
    "theme_color_mode": "auto",
    "theme_nav_links": [
      {
        "title": "Examples",
        "url": "#",
        "children": [
          {
            "title": "Blog",
            "subtitle": "A blog example",
            "url": "/blog/"
          },
          {
            "title": "Podcast",
            "subtitle": "A podcast example",
            "url": "/podcast/"
          }
        ]
      },
      {
        "title": "GitHub",
        "url": "https://github.com/typlog/theme-ueno",
        "blank": true
      }
    ],
    "theme_nav_socials": [
      {
        "icon": "simple-icons:github",
        "title": "GitHub",
        "url": "https://github.com/typlog/theme-nezu",
        "blank": true
      }
    ],
    "theme_podcast_links": [
      {
        "icon": "simple-icons:applepodcasts",
        "title": "Apple Podcasts",
        "url": "https://podcasts.apple.com/us/podcast/typlog/id1564550000",
        "blank": true
      },
      {
        "icon": "simple-icons:spotify",
        "title": "Spotify",
        "url": "https://open.spotify.com/show/5Z6Z9Z6Z6Z6Z6Z6Z6Z6Z6Z",
        "blank": true
      }
    ],
    "theme_copyright": "Copy &copy; 2024, <a href='https://typlog.com/'>Typlog</a>"
  },
  "variables": {
    "color_mode": {
      "type": "enum",
      "default": "auto",
      "choices": ["auto", "light", "dark"]
    },
    "nav_links": {
      "type": "object[]",
      "properties": {
        "title": {
          "type": "string",
          "required": true
        },
        "url": {
          "type": "string",
          "required": true
        },
        "blank": {
          "type": "boolean"
        },
        "children": {
          "type": "object[]",
          "properties": {
            "title": {
              "type": "string",
              "required": true
            },
            "subtitle": {
              "type": "string",
              "required": true
            },
            "url": {
              "type": "string",
              "required": true
            },
            "blank": {
              "type": "boolean"
            }
          }
        }
      }
    },
    "podcast_links": {
      "type": "object[]",
      "properties": {
        "icon": {
          "type": "icon"
        },
        "title": {
          "type": "string",
          "required": true
        },
        "url": {
          "type": "url",
          "required": true
        },
        "blank": {
          "type": "boolean"
        }
      }
    },
    "nav_socials": {
      "type": "object[]",
      "properties": {
        "icon": {
          "type": "icon",
          "required": true
        },
        "title": {
          "type": "string",
          "required": true
        },
        "url": {
          "type": "url",
          "required": true
        },
        "blank": {
          "type": "boolean"
        }
      }
    },
    "copyright": {
      "type": "string"
    }
  },
  "messages": {
    "none": "There is no items",
    "none#zh": "没有内容",
    "archive": "Archive",
    "archive#zh": "归档",
    "prev": "Previous",
    "prev#zh": "上一页",
    "next": "Next",
    "next#zh": "下一页",
    "readthis": "Read This",
    "readthis#zh": "下一篇",
    "switch_auto": "Switch to auto color mode",
    "switch_auto#zh": "切换至系统默认模式",
    "switch_light": "Switch to light color mode",
    "switch_light#zh": "切换至明亮模式",
    "switch_dark": "Switch to dark color mode",
    "switch_dark#zh": "切换至黑暗模式",
    "copy": "Copy",
    "copy#zh": "复制"
  }
}
